<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.SharedPrefsExplorer" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/daytona</item>
        <item name="colorPrimaryVariant">@color/daytona_dark</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/velvet</item>
        <item name="colorSecondaryVariant">@color/velvet_dark</item>
        <item name="colorOnSecondary">@color/white</item>

        <item name="colorTertiary">@color/velvet_light</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
</resources>